{
  "name": "analysta-project",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@9.11.0",
  "scripts": {
    "dev:api": "pnpm --filter api start:dev",
    "dev:web": "pnpm --filter web dev",
    "dev": "concurrently -n api,web -c cyan,magenta \"pnpm dev:api\" \"pnpm dev:web\"",
    "build": "pnpm -r build",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "seed": "pnpm --filter api seed"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
